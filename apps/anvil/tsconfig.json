{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/": ["./src/*"],
      "@/*": ["./src/*"],
      "@dbschema/*": ["./dbschema/*"]
    },
    "incremental": true,
    "skipLibCheck": true,
    "strict": true
  },
}
