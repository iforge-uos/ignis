{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "target": "ES2021",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "paths": {
        "@/*": ["../../apps/anvil/src/*"],
        "@dbschema/*": ["../../apps/anvil/dbschema/*"]
      },
      "incremental": true,
      "skipLibCheck": true,
      "strict": true,
    },
  }